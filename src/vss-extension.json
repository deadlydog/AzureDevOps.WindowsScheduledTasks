{
  "manifestVersion": 1,
  "id": "WindowsScheduledTasksBuildAndReleaseTasks",
  "name": "Windows Scheduled Tasks",
  "version": "0.0.30",
  "publisher": "deadlydog",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Tasks for installing and uninstalling Windows Scheduled Tasks.",
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "utility",
    "task",
    "schedule",
    "scheduler",
    "scheduled task",
    "task scheduler",
    "install",
    "uninstall"
  ],
  "content": {
    "details": {
      "path": "MarketplaceDocs/Overview.md"
    }
  },
  "links": {
    "support": {
      "uri": "https://github.com/deadlydog/AzureDevOps.WindowsScheduledTasks/blob/master/ReadMe.md"
    },
    "license": {
      "uri": "https://github.com/deadlydog/AzureDevOps.WindowsScheduledTasks/blob/master/License.md"
    }
  },
  "icons": {
    "default": "Images/ExtensionIcon.png"
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/deadlydog/AzureDevOps.WindowsScheduledTasks"
  },
  "files": [
    {
      "path": "Tasks/InstallWindowsScheduledTask"
    },
    {
      "path": "Tasks/UninstallWindowsScheduledTask"
    }
  ],
  "contributions": [
    {
      "id": "InstallWindowsScheduledTask",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/InstallWindowsScheduledTask"
      }
    },
    {
      "id": "UninstallWindowsScheduledTask",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/UninstallWindowsScheduledTask"
      }
    }
  ],
  "_internalCommentForMyself": [
    {
      "Absent": "When the \u0027galleryFlags\u0027 element is missing, this extension is only visible to the publisher. When ready, move the `galleryFlags` section out of this \u0027_comment\u0027 section.",
      "Preview": "Will put this extension on the marketplace, but mark it as beta.",
      "Public": "Will make this extension available on the marketplace as ready."
    },
    {
      "galleryFlags": [
        "Preview"
      ]
    },
    {
      "galleryFlags": [
        "Public"
      ]
    }
  ]
}
