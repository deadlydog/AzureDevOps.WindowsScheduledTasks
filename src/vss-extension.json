{
  "manifestVersion": 1,
  "id": "WindowsScheduledTasksBuildAndReleaseTasks",
  "name": "Windows Scheduled Tasks",
  "version": "0.0.0",
  "publisher": "deadlydog",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Manage Windows Scheduled Tasks (install, uninstall, enable, disable). Requires Windows 8 / Windows Server 2012 or greater.",
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "utility",
    "task",
    "schedule",
    "scheduler",
    "scheduled task",
    "task scheduler",
    "install",
		"uninstall",
		"enable",
		"disable"
  ],
  "content": {
    "details": {
      "path": "MarketplaceReadMe.md"
    }
  },
  "links": {
    "support": {
      "uri": "https://github.com/deadlydog/AzureDevOps.WindowsScheduledTasks/blob/master/ReadMe.md"
    },
    "license": {
      "uri": "https://github.com/deadlydog/AzureDevOps.WindowsScheduledTasks/blob/master/License.md"
    }
  },
  "icons": {
    "default": "Images/ExtensionIcon.png"
  },
  "screenshots": [
    {
      "path": "Images/AllTasks.png"
    },
    {
      "path": "Images/InstallTaskSettings.png"
    },
    {
      "path": "Images/UninstallTaskSettings.png"
    }
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/deadlydog/AzureDevOps.WindowsScheduledTasks"
	},
	"badges": [{
			"href": "https://dev.azure.com/deadlydog/OpenSource/_build/latest?definitionId=19&branchName=master",
			"uri": "https://dev.azure.com/deadlydog/OpenSource/_apis/build/status/AzureDevOps.WindowsScheduledTasks?branchName=master",
			"description": "Azure DevOps build for the project"
		},
		{
			"href": "https://dev.azure.com/deadlydog/OpenSource/_release?view=all&definitionId=1",
			"uri": "https://vsrm.dev.azure.com/deadlydog/_apis/public/Release/badge/baf297a4-1582-49bd-b9ca-6d38492faafa/1/1",
			"description": "Azure DevOps deployment for the project"
		}
	],
  "files": [
    {
      "path": "Images",
      "addressable": true
    },
    {
      "path": "Tasks"
    }
  ],
  "contributions": [
    {
      "id": "InstallWindowsScheduledTask",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/InstallWindowsScheduledTask"
      }
    },
    {
      "id": "UninstallWindowsScheduledTask",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/UninstallWindowsScheduledTask"
      }
    },
    {
      "id": "EnableWindowsScheduledTask",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/EnableWindowsScheduledTask"
      }
    },
    {
      "id": "DisableWindowsScheduledTask",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/DisableWindowsScheduledTask"
      }
		},
		{
    	"id": "StartWindowsScheduledTask",
    	"type": "ms.vss-distributed-task.task",
    	"targets": [
    		"ms.vss-distributed-task.tasks"
    	],
    	"properties": {
    		"name": "Tasks/StartWindowsScheduledTask"
    	}
		},
		{
    	"id": "StopWindowsScheduledTask",
    	"type": "ms.vss-distributed-task.task",
    	"targets": [
    		"ms.vss-distributed-task.tasks"
    	],
    	"properties": {
    		"name": "Tasks/StopWindowsScheduledTask"
    	}
    }
	],
	"galleryFlags": [
		"Public"
	]
}
