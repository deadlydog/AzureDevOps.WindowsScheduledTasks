{
    "manifestVersion": 1,
    "id": "WindowsScheduledTasksBuildAndReleaseTasks",
    "name": "Windows Scheduled Tasks",
    "version": "0.0.1",
    "publisher": "deadlydog",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Tasks for installing and uninstalling Windows Scheduled Tasks.",
    "categories": [
        "Build and release"
    ],
    "tags": [
        "utility",
        "task",
        "scheduled task",
        "task scheduler"
    ],
    "content": {
        "details": {
            "path": "MarketplaceDocs/Overview.md"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/deadlydog/VSTS.WindowsScheduledTasks/blob/master/ReadMe.md"
        },
        "issues": {
            "uri": "https://github.com/deadlydog/VSTS.WindowsScheduledTasks/issues"
        },
        "repository": {
            "uri": "https://github.com/deadlydog/VSTS.WindowsScheduledTasks"
        },
        "license": {
            "uri": "https://github.com/deadlydog/VSTS.WindowsScheduledTasks/blob/master/License.md"
        }
    },
    "icons": {
        "default": "Images/ExtensionIcon.png"
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/deadlydog/VSTS.WindowsScheduledTasks"
    },
    "files": [
        {
            "path": "Tasks/InstallWindowsScheduledTask"
        },
        {
            "path": "Tasks/UninstallWindowsScheduledTask"
        }
    ],
    "contributions": [
        {
            "id": "InstallWindowsScheduledTask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/InstallWindowsScheduledTask"
            }
        },
        {
            "id": "UninstallWindowsScheduledTask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/UninstallWindowsScheduledTask"
            }
        }
    ]
}